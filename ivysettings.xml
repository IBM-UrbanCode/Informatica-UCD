<ivysettings>
    <settings defaultResolver="default"/>
    <resolvers>
        <ibiblio name="my-maven" m2compatible="true" root="https://repo1.maven.org/maven2/"/>
        <ibiblio name="central" m2compatible="true"/>

        <chain name="default">
            <resolver ref="central"/>
            <resolver ref="my-maven"/>
        </chain>
    </resolvers>
</ivysettings>